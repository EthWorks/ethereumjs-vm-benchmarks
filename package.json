{
  "name": "ethereumjs-vm-profiler",
  "private": true,
  "scripts": {
    "start": "./scripts/benchmarkSeparately.sh",
    "start:dirty": "INITIAL_STATE=dirty ./scripts/benchmarkSeparately.sh",
    "benchmark": "node benchmarks/index.js",
    "prebuild": "rm -rf dist",
    "build": "tsc -p tsconfig.build.json",
    "verdaccio:run": "verdaccio --config verdaccio/config.yaml",
    "verdaccio:prepare": "rm -rf verdaccio/storage && cp -R verdaccio/prepared-storage verdaccio/storage",
    "verdaccio:install": "npm install --registry 'http://localhost:4873'"
  },
  "dependencies": {
    "@nomiclabs/ethereumjs-vm": "^4.1.1",
    "ethereumjs-account": "^3.0.0",
    "ethereumjs-block": "^2.2.1",
    "ethereumjs-blockchain": "^4.0.2",
    "ethereumjs-common": "^1.4.0",
    "ethereumjs-tx": "^2.1.1",
    "ethereumjs-util": "^6.2.0",
    "ethers": "^4.0.0",
    "lodash": "^4.17.15",
    "mathjs": "^7.0.0",
    "pretty-time": "^1.1.0",
    "ts-essentials": "^6.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/chai-as-promised": "^7.1.2",
    "@types/lodash": "^4.14.149",
    "@types/lru-cache": "^5.1.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^14.0.5",
    "@types/sinon": "^7.5.1",
    "@types/sinon-chai": "^3.2.3",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.8.0",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "mocha": "^6.2.2",
    "sinon": "^8.0.1",
    "sinon-chai": "^3.3.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  }
}
