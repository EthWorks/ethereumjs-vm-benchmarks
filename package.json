{
  "name": "ethereumjs-vm-profiler",
  "private": true,
  "scripts": {
    "start": "./scripts/benchmarkSeparately.sh",
    "start:dirty": "INITIAL_STATE=dirty ./scripts/benchmarkSeparately.sh",
    "benchmark": "node benchmarks/index.js",
    "inspect": "node --inspect-brk benchmarks/index.js",
    "verdaccio:run": "verdaccio --config verdaccio/config.yaml",
    "verdaccio:prepare": "rm -rf verdaccio/storage && cp -R verdaccio/prepared-storage verdaccio/storage",
    "verdaccio:install": "npm install --registry 'http://localhost:4873'"
  },
  "dependencies": {
    "ethers": "^4.0.0",
    "mathjs": "^7.0.0",
    "pretty-time": "^1.1.0"
  }
}
